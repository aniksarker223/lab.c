#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100];
    int freq1[256]={0}, freq2[256]={0}, i;

    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);

    for(i=0; a[i]; i++) freq1[(unsigned char)a[i]]++;
    for(i=0; b[i]; i++) freq2[(unsigned char)b[i]]++;

    for(i=0; i<256; i++){
        if(freq1[i] != freq2[i]){
            printf("Not Anagram\n");
            return 0;
        }
    }

    printf("Anagram Strings\n");
    return 0;
}
