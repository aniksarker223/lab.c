#include <stdio.h>
#include <string.h>
struct st{ char name[50]; int age; };
int main(){
    int n; scanf("%d",&n);
    struct st a[n], t;
    for(int i=0;i<n;i++) scanf("%s%d",a[i].name,&a[i].age);
    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
            if(strcmp(a[i].name,a[j].name)>0){
                t=a[i]; a[i]=a[j]; a[j]=t;
            }
    for(int i=0;i<n;i++) printf("%s %d\n",a[i].name,a[i].age);
    return 0;
}
