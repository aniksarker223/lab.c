#include <stdio.h>

int main() {
    char s[100];
    int i, len = 0, flag = 0;

    printf("Enter a string: ");
    gets(s);

    while (s[len] != '\0')
        len++;

    for (i = 0; i < len/2; i++) {
        if (s[i] != s[len - 1 - i]) {
            flag = 1;
            break;
        }
    }

    if (flag == 0)
        printf("Palindrome String\n");
    else
        printf("Not Palindrome\n");

    return 0;
}
