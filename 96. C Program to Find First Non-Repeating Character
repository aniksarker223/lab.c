#include <stdio.h>

int main() {
    char s[200];
    int freq[256]={0}, i;

    printf("Enter string: ");
    gets(s);

    for(i=0; s[i]; i++)
        freq[(unsigned char)s[i]]++;

    for(i=0; s[i]; i++){
        if(freq[(unsigned char)s[i]] == 1){
            printf("First non-repeating = %c\n", s[i]);
            return 0;
        }
    }

    printf("No unique character found\n");

    return 0;
}
