#include <stdio.h>

int main() {
    char s[100], out[100];
    int i, j = 0;

    printf("Enter a string: ");
    gets(s);

    for(i=0; s[i]; i++){
        if(!(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||
             s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U'))
            out[j++] = s[i];
    }
    out[j] = '\0';

    printf("String without vowels = %s\n", out);

    return 0;
}
