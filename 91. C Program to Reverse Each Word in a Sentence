#include <stdio.h>

void reverse(char s[], int start, int end){
    while(start < end){
        char t = s[start];
        s[start] = s[end];
        s[end] = t;
        start++;
        end--;
    }
}

int main() {
    char s[200];
    int i = 0, start = 0;

    printf("Enter sentence: ");
    gets(s);

    while(s[i]){
        if(s[i]==' '){
            reverse(s, start, i-1);
            start = i+1;
        }
        i++;
    }

    reverse(s, start, i-1);

    printf("Output: %s\n", s);

    return 0;
}
