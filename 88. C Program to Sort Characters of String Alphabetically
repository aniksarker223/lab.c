#include <stdio.h>
#include <string.h>

int main() {
    char s[100], temp;
    int i, j;

    printf("Enter string: ");
    gets(s);

    for(i=0; s[i]; i++){
        for(j=i+1; s[j]; j++){
            if(s[i] > s[j]){
                temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
        }
    }

    printf("Sorted String = %s\n", s);

    return 0;
}
