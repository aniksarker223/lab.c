#include <stdio.h>

int main() {
    char s[200];
    int i, up=0, low=0;

    printf("Enter string: ");
    gets(s);

    for(i=0; s[i]; i++){
        if(s[i]>='A' && s[i]<='Z') up++;
        else if(s[i]>='a' && s[i]<='z') low++;
    }

    printf("Uppercase = %d\nLowercase = %d\n", up, low);

    return 0;
}
