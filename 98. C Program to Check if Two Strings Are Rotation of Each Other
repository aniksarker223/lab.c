#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100], temp[200];

    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);

    strcpy(temp, a);
    strcat(temp, a);

    if(strstr(temp, b))
        printf("Rotation String\n");
    else
        printf("Not Rotation\n");

    return 0;
}
