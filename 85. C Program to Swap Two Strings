#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100], temp[100];

    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);

    strcpy(temp, a);
    strcpy(a, b);
    strcpy(b, temp);

    printf("After Swap:\n");
    printf("A = %s\n", a);
    printf("B = %s\n", b);

    return 0;
}
