#include <stdio.h>
struct st{ int id, age; };
int main(){
    int n; scanf("%d",&n);
    struct st a[n], t;
    for(int i=0;i<n;i++) scanf("%d%d",&a[i].id,&a[i].age);
    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
            if(a[i].age > a[j].age){
                t=a[i]; a[i]=a[j]; a[j]=t;
            }
    for(int i=0;i<n;i++) printf("%d %d\n", a[i].id, a[i].age);
    return 0;
}
