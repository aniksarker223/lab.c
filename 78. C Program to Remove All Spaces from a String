#include <stdio.h>

int main() {
    char s[100], out[100];
    int i, j = 0;

    printf("Enter a string: ");
    gets(s);

    for (i = 0; s[i] != '\0'; i++) {
        if (s[i] != ' ')
            out[j++] = s[i];
    }
    out[j] = '\0';

    printf("Without spaces: %s\n", out);

    return 0;
}
