#include <stdio.h>

int main() {
    char s[100], ch;
    int i, count = 0;

    printf("Enter string: ");
    gets(s);

    printf("Enter character to check: ");
    scanf("%c", &ch);

    for(i=0; s[i]; i++){
        if(s[i] == ch)
            count++;
    }

    printf("Frequency of %c = %d\n", ch, count);

    return 0;
}
