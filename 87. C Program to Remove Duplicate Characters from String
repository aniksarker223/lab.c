#include <stdio.h>

int main() {
    char s[100];
    int freq[256] = {0}, i;

    printf("Enter string: ");
    gets(s);

    printf("After removing duplicates: ");
    for(i=0; s[i]; i++){
        if(freq[(unsigned char)s[i]] == 0){
            printf("%c", s[i]);
            freq[(unsigned char)s[i]] = 1;
        }
    }

    printf("\n");
    return 0;
}
