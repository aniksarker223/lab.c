#include <stdio.h>

int main() {
    char s[300];
    int freq[26] = {0}, i;

    printf("Enter sentence: ");
    gets(s);

    for(i=0; s[i]; i++){
        if(s[i]>='a'&&s[i]<='z')
            freq[s[i]-'a'] = 1;
        else if(s[i]>='A'&&s[i]<='Z')
            freq[s[i]-'A'] = 1;
    }

    for(i=0; i<26; i++){
        if(freq[i] == 0){
            printf("Not Pangram\n");
            return 0;
        }
    }

    printf("Pangram Sentence\n");
    return 0;
}
