#include <stdio.h>

int main() {
    char s[100];
    int freq[256] = {0}, i;

    printf("Enter a string: ");
    gets(s);

    for (i = 0; s[i] != '\0'; i++) {
        freq[(unsigned char)s[i]]++;
    }

    for (i = 0; i < 256; i++) {
        if (freq[i] > 0)
            printf("%c = %d\n", i, freq[i]);
    }

    return 0;
}
